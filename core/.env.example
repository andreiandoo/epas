APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# ============================================================================
# SEATING MAPS MODULE
# ============================================================================

# Enable/disable seating maps functionality globally
SEATING_ENABLED=true

# Seat hold time-to-live in seconds (default: 600 = 10 minutes)
SEATING_HOLD_TTL_SECONDS=600

# Maximum seats a single session can hold simultaneously
SEATING_MAX_HELD_PER_SESSION=10

# Rate limits (requests per minute)
SEATING_RATE_LIMIT_HOLD=30
SEATING_RATE_LIMIT_CONFIRM=10
SEATING_RATE_LIMIT_RELEASE=30
SEATING_RATE_LIMIT_QUERY=60

# Use Redis for seat holds (recommended for production)
SEATING_USE_REDIS=true
SEATING_REDIS_CONNECTION=default
SEATING_REDIS_PREFIX=seating

# Storage disk for background images and exports
SEATING_STORAGE_DISK=public

# Background image constraints
SEATING_BG_MAX_SIZE_MB=5

# CORS origins for public seating APIs (comma-separated domains)
SEATING_CORS_ORIGINS=*

# Logging
SEATING_LOG_CHANNEL=stack
SEATING_LOG_LEVEL=info
SEATING_LOG_QUERIES=false

# Events & webhooks
SEATING_EVENTS_ENABLED=true

# Widget configuration
SEATING_WIDGET_VERSION=1.0.0
SEATING_WIDGET_CDN=

# ============================================================================
# DYNAMIC PRICING MODULE
# ============================================================================

# Enable/disable dynamic pricing functionality
SEATING_DP_ENABLED=false

# Cooldown between price changes for same seat (seconds)
SEATING_DP_COOLDOWN_SECONDS=120

# Enforce floor/ceiling price limits
SEATING_DP_ENFORCE_LIMITS=true

# Maximum price change percentage per update
SEATING_DP_MAX_CHANGE_PERCENT=50

# Recompute interval for pricing strategies (minutes)
SEATING_DP_RECOMPUTE_INTERVAL=5

# Cache TTL for computed prices (seconds)
SEATING_DP_CACHE_TTL=60

# ============================================================================
# SEATING MAINTENANCE
# ============================================================================

# Purge hold records older than X days
SEATING_PURGE_HOLDS_DAYS=30

# Archive old event seating layouts after X days
SEATING_ARCHIVE_DAYS=365
